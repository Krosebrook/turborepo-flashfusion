# FlashFusion Environment Variables Template
# Copy this file to .env and fill in your actual values

# AI Services
E2B_API_KEY=your_e2b_api_key_here
ANTHROPIC_API_KEY=sk-ant-your-key-here
OPENAI_API_KEY=sk-your-key-here
FIRECRAWL_API_KEY=fc-your-key-here

# Database
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
DATABASE_URL=your_database_url_here

# PostgreSQL (for local development)
POSTGRES_PASSWORD=flashfusion_dev

# Redis
REDIS_URL=redis://localhost:6379

# Monitoring
GRAFANA_PASSWORD=admin

# CI/CD
TURBO_TOKEN=your_turbo_token_here
TURBO_TEAM=your_turbo_team_here

# Notifications
SLACK_WEBHOOK_URL=your_slack_webhook_url_here

# Security
GITHUB_TOKEN=your_github_token_here
GH_RUNNER_TOKEN=your_runner_token_here

# Kubernetes (base64 encoded kubeconfig)
KUBECONFIG_STAGING=your_staging_kubeconfig_base64
KUBECONFIG_PRODUCTION=your_production_kubeconfig_base64
GROQ_API_KEY=your-groq-key-here
GEMINI_API_KEY=your-gemini-key-here

# GitHub Integration (for repository discovery)
GITHUB_TOKEN=ghp_your_github_token_here

# Supabase (optional for usage tracking)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-key-here

# Development settings
NODE_ENV=development
PORT=8080