# FlashFusion Open Lovable Configuration
# Copy this file to .env and fill in your actual API keys

# Required for Open Lovable functionality
E2B_API_KEY=your_e2b_api_key_here
ANTHROPIC_API_KEY=sk-ant-your-key-here
OPENAI_API_KEY=sk-your-key-here

# Optional but recommended
FIRECRAWL_API_KEY=fc-your-key-here
GROQ_API_KEY=your-groq-key-here
GEMINI_API_KEY=your-gemini-key-here

# GitHub Integration (for repository discovery)
GITHUB_TOKEN=ghp_your_github_token_here

# Supabase (optional for usage tracking)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-key-here

# RAG System Configuration (optional)
# RAG_EMBEDDING_MODEL=text-embedding-3-small
# RAG_GENERATION_MODEL=gpt-4
# RAG_CHUNK_SIZE=1000
# RAG_CHUNK_OVERLAP=200

# Development settings
NODE_ENV=development
PORT=8080