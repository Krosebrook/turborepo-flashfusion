{
  "name": "turborepo-flashfusion",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "turbo format",
    "test": "turbo test",
    "type-check": "turbo type-check",
    "clean": "turbo clean",
    "restore-state": "node restore-session.js",
    "next-steps": "node restore-session.js",
    "deploy": "vercel --prod",
    "ff": "node tools/cli/ff-cli.js",
    "security:scan": "node tools/security/consolidated-reporter.js",
    "security:npm": "node tools/security/npm-audit-scanner.js",
    "security:dast": "node tools/security/zap-dast-scanner.js",
    "security:install-zap": "docker pull ghcr.io/zaproxy/zaproxy:stable"
  },
  "devDependencies": {
    "turbo": "^2.3.0",
    "@turbo/gen": "^2.3.0",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "packageManager": "npm@10.0.0",
  "workspaces": [
    "apps/*",
    "packages/*",
    "tools/*"
  ]
}
