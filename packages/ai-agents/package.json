{
  "name": "@flashfusion/ai-agents",
  "version": "1.0.0",
  "description": "AI agent implementations and orchestration for FlashFusion",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**",
    "src/**",
    "orchestration/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint \"src/**/*.{js,ts}\" \"orchestration/**/*.{js,ts}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts}\" \"orchestration/**/*.{js,ts}\" --fix",
    "format": "prettier --write \"src/**/*.{js,ts,json,md}\" \"orchestration/**/*.{js,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "@supabase/supabase-js": "^2.0.0",
    "openai": "^4.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "tsup": "^8.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "jest": "^29.0.0"
  }
}